{
    "identifier": "io.barrow.sort",
    "name": "Sort",
    "organization": "Kyle Barrow",
    "description": "Alphanumeric line sorting",
    "version": "1.0",
    "repository": "https://github.com/kylebarrow/Sort.novaextension",
    "bugs": "https://github.com/kylebarrow/Sort.novaextension/issues",
    "categories": ["commands"],   
    "main": "main.js",
    "activationEvents": [
        "onLanguage:javascript"
    ],  
    "entitlements": {},   
    "commands": {
        "editor": [
            {
                "title": "Sort Selection",
                "command": "sort.sortSelection",
                "when": "editorHasFocus && editorHasSelection"
            }
        ]
    }
}
